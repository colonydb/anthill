{
  "name": "@colonydb/anthill",
  "version": "0.1.0",
  "main": "index.js",
  "repository": "git@github.com:colonydb/anthill.git",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rslib build",
    "build:examples": "vite build examples",
    "release": "pnpm version && git push && git push --tags && open \"https://github.com/colonydb/anthill/releases/new?tag=v$(jq '.version' package.json --raw-output)\"",
    "sketchbook:start": "webpack --config sketchbook/webpack.config.js --mode development && pnpm sketchbook start sketchbook/example.json",
    "sketchbook:build": "webpack --config sketchbook/webpack.config.js --mode production && pnpm sketchbook build sketchbook/example.json",
    "test:prettier": "prettier -c src README.md",
    "test": "pnpm build && pnpm test:prettier",
    "dev:examples": "vite examples"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*.css": "./dist/*.css",
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.cjs"
    }
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@rsbuild/plugin-react": "^1.1.1",
    "@rslib/core": "^0.5.4",
    "@sketchbook-js/sketchbook": "^0.3.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/css-modules": "^1.0.5",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "prettier": "^3.5.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2"
  },
  "dependencies": {
    "@haydn/universal": "^0.0.3"
  }
}
